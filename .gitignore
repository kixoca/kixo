# Ignore RubyMine configs
/.idea

# Ignore DS Files
.DS_Store

# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore local environment variables
/.env

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore dynamically uploaded assets
/public/system
